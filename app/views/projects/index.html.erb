<h3><%= link_to 'Create project', new_project_path(@project) %></h3>
<div class="container">
  <h2> Projects: </h2>
  <% @projects.each do |project| %>
    <p> <%=link_to project.name, project %>
    <%= project.summary.truncate(20) %>
    <%= link_to 'delete', project_path(project), method: :delete, data: { confirm: 'Are you sure?' } %></p>
  <% end %>
</div>
